{
  "manifest_version": 2,
  "name": "Jira Sprint Burndown Extension for Subtask",
  "description": "This extension allows the user to generate a Burndown chart from a Spring Board based in subtasks.",
  "version": "1.1.1",
  "author": "Anderson Silva",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "main.html"
  },
  "developer":{
    "name": "Anderson Silva",
    "url": "https://andersonolisilva.github.io/"
  },
  "permissions": [
    "activeTab",
    "storage",
	  "http://*/",
  	"https://*/"
  ],
  "content_security_policy": "script-src 'self' https://ajax.googleapis.com https://code.highcharts.com https://maxcdn.bootstrapcdn.com/; object-src 'self'"
}